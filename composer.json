{
    "name": "cakemanager/cakephp-posttypes",
    "type": "cakephp-plugin",
    "description": "Plugin for CakePHP 3.x to create easy posttypes",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Mulder",
            "email": "bobmulder@outlook.com"
        }
    ],
	"require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "~3.0",
        "cakemanager/cakephp-cakemanager": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": " 4.6.*@dev"
    },
    "autoload": {
        "psr-4": {
            "PostTypes\\": "src"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "PostTypes\\Test\\": "tests",
            "PostTypes\\Test\\Fixture\\": "tests/Fixture",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "App\\": "tests/App"
        }
    },
	"minimum-stability" : "dev"
}
